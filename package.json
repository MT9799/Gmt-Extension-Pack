{
  "name": "gmt",
  "displayName": "Gmt Extension Pack",
  "description": "Syntax highlighting for gmt files of the game Euclidea",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MT9799/Gmt-Syntax-Highlighting"
  },
  "author": "MT9799",
  "keywords": [
    "gmt",
    "syntax",
    "highlighting",
    "Extension"
  ],
  "contributes": {
    "languages": [{
      "id": "gmt",
      "aliases": ["Gmt", "gmt"],
      "extensions": [".gmt"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "gmt",
      "scopeName": "source.gmt",
      "path": "./syntaxes/gmt.tmLanguage.json"
    }],
    "snippets": [{
      "language": "gmt",
      "path": "./snippets/gmt.json"
    }]
  }
}

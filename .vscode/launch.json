// A launch configuration that launches the extension inside a new window
// Hover to view descriptions of existing attributes
{
	"version": "0.2.0",
    "configurations": [
        {
            "name": "Gmt Extension Pack",
            "type": "extensionHost",
            "request": "launch",
            "args": [
                "--extensionDevelopmentPath=${workspaceFolder}"
            ]
        }
    ]
}